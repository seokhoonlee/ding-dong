{
  "manifest_version": 2,

  "name": "DingDong",
  "description": "This extension alerts users when specified content is available at specified url",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon/icon32.png",
    "default_popup": "popup/popup.html"
  },

  "permissions": [
    "notifications",
    "storage",
    "tabs"
  ],

  "icons": {
    "128": "icon/icon128.png"
  }
}